@model webTeste.Models.Contato

@{
    ViewData["Title"] = "Details";
}
<main class="vh-100 ">
<h1>Detalhes</h1>

<div>
    <h4>Contato</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Mensagem)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Mensagem)
        </dd>
    </dl>
</div>
<div>
    <a type="button" class="btn btn-primary" asp-action="Edit" asp-route-id="@Model?.Id_Contato">Edit</a> |
    <a type="button" class="btn btn-primary" asp-controller="Contatos" asp-action="Index">Voltar</a>
</div>
</main>